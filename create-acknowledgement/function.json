{
  "bindings": [
    {
      "name": "myBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "target/{name}.dat",
      "connection": "BATCH_STORAGE"
    },
    {
      "name": "outputSbTopic",
      "type": "serviceBus",
      "topicName": "%ACKNOWLEDGMENT_TOPIC%",
      "connection": "MESSAGE_CONNECTION",
      "direction": "out"
    },
    {
      "name": "myOutputBlob",
      "type": "blob",
      "path": "{blobTrigger}.ack",
      "connection": "BATCH_STORAGE",
      "direction": "out"
    }
  ]
}
